: my $strip_list = $login.get_completed_strips
: $op.setStateVar( $state, 'strip_list', $strip_list )
: $op.setStateVar( $state, 'pag_path', $app_path ~ '/p/mycompleted' )

: if $strip_list.size() > 0 {
    <h2>My Completed Strips</h2>
:   include $op.tmpl( $app_path, 'paginate_strips' )
: } else {
    <h2>No Completed Strips Yet</h2>
: }

